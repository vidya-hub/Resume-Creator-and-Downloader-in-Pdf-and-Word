<% if (skills && skills.length>0) { %>
<tr>
  <td
    class="text-left font-weight-bold"
    style="padding-top: 12px; font-size: 20px; padding-bottom: 12px"
  >
    TECHNICAL SKILLS
  </td>
  <td class="text-right"></td>
</tr>
<tr>
  <td class="text-left" style="width: 100%" colspan="2">
    <ul style="font-size: 19px">
      <% for(var i=0; i < skills.length; i++) { %>
      <li><%=skills[i]%></li>
      <% } %>
    </ul>
  </td>
</tr>
<%}%>
