<% if (objectives && objectives.length>0) { %>
<tr>
  <td style="padding-bottom: 15px">
    <p
      class="text-left font-weight-bold"
      style="padding-top: 12px; font-size: 20px"
    >
      Objectives
    </p>
    <ul style="width: 80em">
      <% for(var i=0; i < objectives.length; i++) { %>
      <li
        style="
          font-size: 19px;

          text-align: justify;

          margin-bottom: 5px;
        "
      >
        <%=objectives[i]%>
      </li>
      <% } %>
    </ul>
  </td>
</tr>
<% } %>
