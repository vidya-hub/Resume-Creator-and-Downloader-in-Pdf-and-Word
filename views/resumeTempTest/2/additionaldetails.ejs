<% if (affiliations && affiliations.length>0) { %>

<div
  class="yui-gf"
  style="margin-bottom: 2em; padding-bottom: 2em; border-bottom: 1px solid #ccc"
>
  <div class="yui-u first">
    <h2 style="font-size: 152%; color: #333">Affiliations</h2>
  </div>
  <div class="yui-u">
    <% for(var i=0; i < affiliations.length; i++) { %>

    <div
      class="job"
      style="position: relative; margin-bottom: 1em; padding-bottom: 1em"
    >
      <h2 style="font-size: 152%; color: #333"><%=affiliations[i]%></h2>
    </div>
    <% } %>
  </div>
</div>
<% } %>
